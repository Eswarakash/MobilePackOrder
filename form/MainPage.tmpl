{{ define "MainPage" }}
  {{ template "Header" }}
    {{ template "Menu" }} 
   <h2> Welcome To Customer Registration Page </h2>  
    <form method="POST" action="insert">
      <label> Name </label><input type="text" name="CustomerName" /><br />
      <label> Number </label><input type="number" name="CustomerNumber" /><br />
      <label> Alternate Number </label><input type="number" name="AlternativeNumber" /><br />
      <label> Dob </label><input type="text" name="CustomerDob" /><br />
      <label> Gender </label><input type="text" name="CustomerGender" /><br />
      <label> Address </label><input type="text" name="CustomerAddress" /><br />
      <label> Pincode </label><input type="int" name="CustomerPincode" /><br />
      
      <input type="submit" value="Save user" />
    </form>
  {{ template "Footer" }}
{{ end }}
